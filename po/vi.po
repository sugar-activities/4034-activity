#: moon.py:218
msgid ""
"Today's Moon Information\n"
"\n"
msgstr ""
"Thông tin về mặt trăng của hôm nay\n"
"\n"

#: moon.py:219
#, python-format
msgid ""
"Phase:\n"
"%s\n"
"\n"
msgstr ""
"Tuần:\n"
"%s\n"
"\n"

#: moon.py:220
#, python-format
msgid ""
"Julian Date:\n"
"%.2f (astronomical)\n"
"\n"
msgstr ""
"Lịch juliêng:\n"
"%.2f (thiên văn)\n"
"\n"

#: moon.py:221
#, python-format
msgid ""
"Age:\n"
"%(days).0f days, %(hours).0f hours, %(minutes).0f minutes\n"
"\n"
msgstr ""
"Thời gian hoạt động:\n"
"%(days).0f ngày, %(hours).0f giờ, %(minutes).0f phút\n"
"\n"

#: moon.py:222
#, python-format
msgid ""
"Lunation:\n"
"%(phase).2f%% through lunation %(lunation)d\n"
"\n"
msgstr ""
"Tháng âm lịch:\n"
"%(phase).2f%% trong tuần trăng %(lunation)d\n"
"\n"

#: moon.py:223
#, python-format
msgid ""
"Surface Visibility:\n"
"%.0f%% (estimated)\n"
"\n"
msgstr ""
"Khả kiến mặt:\n"
"%.0f%% (xấp xỉ)\n"
"\n"

#: moon.py:224
#, python-format
msgid ""
"Selenographic Terminator Longitude:\n"
"%(deg).1f°%(westOrEast)s (%(riseOrSet)s)\n"
"\n"
msgstr ""
"Độ kinh đường chấm dứt thuộc khoa nghiên cứu mặt trăng:\n"
"%(deg).1f°%(westOrEast)s (%(riseOrSet)s)\n"
"\n"

#: moon.py:225
#, python-format
msgid ""
"Next Full Moon:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"Trăng tròn kế tiếp:\n"
"%(date)s trong vòng %(days).0f ngày\n"
"\n"

#: moon.py:226
#, python-format
msgid ""
"Next New Moon:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"Trăng non kế tiếp:\n"
"%(date)s trong vòng %(days).0f ngày\n"
"\n"

#: moon.py:227
#, python-format
msgid ""
"Next Lunar eclipse:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"Che khuất mặt trăng kế tiếp:\n"
"%(date)s trong vòng %(days).0f ngày\n"
"\n"

#: moon.py:228
#, python-format
msgid ""
"Next Solar eclipse:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"Che khuất mặt trời kế tiếp:\n"
"%(date)s trong vòng %(days).0f ngày\n"
"\n"

